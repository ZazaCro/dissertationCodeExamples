FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 101325;

boundaryField
{
    walls
    {
        type            calculated;
        value           $internalField;
    }
    maxY
    {
        type            calculated;
        value           $internalField;
    }
    fluid_to_solid
    {
        type            calculated;
        value           $internalField;
    }
}


// ************************************************************************* //
